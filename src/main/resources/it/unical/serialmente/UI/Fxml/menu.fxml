<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>


<HBox xmlns="http://javafx.com/javafx/25"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="it.unical.serialmente.UI.Controller.MenuController"
      spacing="20"
      style="-fx-background-color: #2c2c2c; -fx-padding: 10;">
        <Button fx:id="film" mnemonicParsing="false" text="Film"/>
        <Button fx:id="serieTV" mnemonicParsing="false" text="Serie TV"/>
        <Button fx:id="watchlist" mnemonicParsing="false" text="Watchlist"/>
        <Button fx:id="areaUtente" mnemonicParsing="false" text="Area Utente"/>
        <Button fx:id="logout" mnemonicParsing="false" text="Logout"/>
</HBox>