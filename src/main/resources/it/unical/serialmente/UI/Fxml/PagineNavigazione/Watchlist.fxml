<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@../../CSS/watchlist.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unical.serialmente.UI.Controller.PagineNavigazione.ControllerWatchlist" styleClass="root-watchlist">
    <center>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER" styleClass="contenitore-lista">
            <ListView fx:id="listTitoli" focusTraversable="false" styleClass="lista-titoli"/>
            <Button fx:id="btnSfigliaTitoli" mnemonicParsing="false" prefHeight="26.0" prefWidth="181.0"
                    text="Sfoglia tutti i titoli" styleClass="btn-sfoglia"/>
        </VBox>
    </center>
</BorderPane>